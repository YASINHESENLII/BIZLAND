@using BIZLAND.ViewModels;
@model RegisterVM
<!DOCTYPE html>
<html>
<head>
<title>Creative Colorlib SignUp Form</title>
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
<!-- Custom Theme files -->
<link href="~/assets/css/register.css" rel="stylesheet" type="text/css" media="all" />
<!-- //Custom Theme files -->
<!-- web font -->
<link href="~/assets/css/register.css">
<!-- //web font -->
</head>
<body>
	<!-- main -->
	<div class="main-w3layouts wrapper">
		<h1>Creative SignUp Form</h1>
		<div class="main-agileinfo">
			<div class="agileits-top">
				<form action="#" method="post">
					<input asp-for="Name" placeholder="Name" >
					<span asp-validation-for="Name"> </span>
					<br />
					<input asp-for="Surname" placeholder="Surname" required="">
					<span asp-validation-for="Surname"> </span>
					<br />
					<input asp-for="Usename" placeholder="Surname" required="">
					<span asp-validation-for="Usename"> </span><br />
					<input asp-for="Email" placeholder="Surname" required="">
					<span asp-validation-for="Email"> </span><br />
					<input asp-for="Password" placeholder="Surname" required="">
					<span asp-validation-for="Password"> </span><br />
					<input asp-for="ConfirmPaswwod" placeholder="Surname" required="">
					<span asp-validation-for="ConfirmPaswwod"> </span><br />
					<div asp-validation-summary="ModelOnly" class="badge-outline-danger"></div>
					
					
					<input type="submit" value="SIGNUP">
				</form>
			</div>
		</div>
		<!-- copyright -->
		<div class="colorlibcopy-agile">
			<p>© 2018 Colorlib Signup Form. All rights reserved | Design by <a href="~/assets/css/register.css">Colorlib</a></p>
		</div>
		<!-- //copyright -->
		<ul class="colorlib-bubbles">
			<li></li>
			<li></li>
			<li></li>a
			<li></li>
			<li></li>
			<li></li>
			<li></li>
			<li></li>
			<li></li>
			<li></li>
		</ul>
	</div>
	<!-- //main -->
</body>
</html>