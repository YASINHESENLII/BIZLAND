@using BIZLAND.ViewModels;
@model LoginVM
<!DOCTYPE html>
<html>
<head>
<title>Creative Colorlib SignUp Form</title>
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
<!-- Custom Theme files -->
<link href="~/assets/css/register.css" rel="stylesheet" type="text/css" media="all" />
<!-- //Custom Theme files -->
<!-- web font -->
<link href="~/assets/css/register.css">
<!-- //web font -->
</head>
<body>
	<!-- main -->
	<div class="main-w3layouts wrapper">
		<h1>Creative SignUp Form</h1>
		<div class="main-agileinfo">
			<div class="agileits-top">
				<form action="#" method="post">
					<input asp-for="UsernameOrEmail" class="text"  placeholder="UsernameOrEamil" required="">
				<span asp-validation-for="UsernameOrEmail" ></span>

					<br />
					<input asp-for="Password" class="text"  placeholder="Password" required="">
					<span asp-validation-for="Password" ></span>
					<br />
		     	<input asp-for="RememberMe"   placeholder="Password" required="">
					<span asp-validation-for="RememberMe" ></span>
					<br />
					<div style="color:red" asp-validation-summary="ModelOnly"></div>
					
						<button type="submit"
                          
                        >
                          Login
                        </button>
					
				</form>
				<p>Don't have an Account? <a href="#"> Login Now!</a></p>
			</div>
		</div>
		<!-- copyright -->
		<div class="colorlibcopy-agile">
			<p>© 2018 Colorlib Signup Form. All rights reserved | Design by <a href="~/assets/css/register.css">Colorlib</a></p>
		</div>
		<!-- //copyright -->
		<ul class="colorlib-bubbles">
			<li></li>
			<li></li>
			<li></li>a
			<li></li>
			<li></li>
			<li></li>
			<li></li>
			<li></li>
			<li></li>
			<li></li>
		</ul>
	</div>
	<!-- //main -->
</body>
</html>